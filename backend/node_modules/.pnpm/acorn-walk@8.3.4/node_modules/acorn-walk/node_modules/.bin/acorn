#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/bin/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn@8.14.1/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/node_modules:/Users/daria.chiriac/Desktop/node_modules:/Users/daria.chiriac/node_modules:/Users/node_modules:/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn-walk@8.3.4/node_modules/acorn-walk/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn-walk@8.3.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/bin/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn@8.14.1/node_modules/acorn/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn@8.14.1/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/node_modules:/Users/daria.chiriac/Desktop/node_modules:/Users/daria.chiriac/node_modules:/Users/node_modules:/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn-walk@8.3.4/node_modules/acorn-walk/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/acorn-walk@8.3.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.14.1/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.14.1/node_modules/acorn/bin/acorn" "$@"
fi
