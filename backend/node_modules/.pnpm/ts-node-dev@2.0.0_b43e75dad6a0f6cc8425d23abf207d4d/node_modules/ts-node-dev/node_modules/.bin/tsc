#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/bin/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/typescript@5.8.3/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/node_modules:/Users/daria.chiriac/Desktop/node_modules:/Users/daria.chiriac/node_modules:/Users/node_modules:/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/ts-node-dev@2.0.0_b43e75dad6a0f6cc8425d23abf207d4d/node_modules/ts-node-dev/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/ts-node-dev@2.0.0_b43e75dad6a0f6cc8425d23abf207d4d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/bin/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/typescript@5.8.3/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/node_modules:/Users/daria.chiriac/Desktop/node_modules:/Users/daria.chiriac/node_modules:/Users/node_modules:/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/ts-node-dev@2.0.0_b43e75dad6a0f6cc8425d23abf207d4d/node_modules/ts-node-dev/node_modules:/Users/daria.chiriac/Desktop/proiect-seti/backend/node_modules/.pnpm/ts-node-dev@2.0.0_b43e75dad6a0f6cc8425d23abf207d4d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.8.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.8.3/node_modules/typescript/bin/tsc" "$@"
fi
